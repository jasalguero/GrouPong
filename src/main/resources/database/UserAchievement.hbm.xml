<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.groupon.hcktn.groupong.model.entity.UserAchievement" table="user_achievement">
        <id name="id" column="id" type="integer">
            <generator class="native"></generator>
        </id>
        <property name="userId" column="user_id" type="integer" not-null="true"></property>
        <property name="achievementId" column="achievement_id" type="integer" not-null="true"></property>
    </class>
</hibernate-mapping>